<?xml version="1.0" encoding="UTF8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.project.project_backend.mapper.LabelMapper">

    <select id="getAllLabel" resultType="String">
        select label_name from label
    </select>

    <select id="getLabelByLike" resultType="String">
        select label_name from label where label_name like #{labelName}
    </select>

    <select id="getLabelByLink" resultType="String">
        select label_name from label where link_id=(select id from file where link like #{link});
    </select>


</mapper>